<%- include('../partials/header') %> 

<form action="">
  <input type="search" name="searchWods" id="searchWods" aria-label="Search wods for keyword activity">
  <button>Search</button>
</form>

<table class="table table-striped table-bordered table-responsive-sm">
    <thead>
      <tr>
        <th scope="col">Wod</th>
        <th scope="col">Type</th>
        <th scope="col">Style</th>
        <th scope="col">The Work</th>
        <th scope="col">Modifications</th>
        <th scope="col">Duration</th>
        <th></th>
        </tr>
    </thead>
    <tbody>
      <% wods.forEach((w) => { %>
        <tr scope="row">
          <td><%= w.title %></td>
          <td><%= w.type %></td>
          <td><%= w.style %></td> 
          <td><%= w.movements %></td>
          <td><%= w.modifications %></td>
          <td><%= w.duration %></td>
          <td><a id="results" href="/results/<%= w._id %>">Details</a></td>
          <!-- <form type="submit" action="/results/<%= w._id %>" method="GET"><td><button class="btn btn-dark">Details</button></td></form> -->
        </tr>
      <% }) %>
    </tbody>
  </table>
  
     <!-- Need to search wods by keyword -->

<%- include('../partials/footer') %> 